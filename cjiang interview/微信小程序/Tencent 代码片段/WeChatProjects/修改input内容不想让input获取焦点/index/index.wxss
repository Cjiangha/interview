.count_control{height:60rpx;line-height:60rpx;background:#f8f8f8;border-radius:10rpx;width:252rpx;position:absolute;right:0;top:20rpx;}
.count_control em,.count_control input{float:left;}
.count_control em{width:80rpx;height:60rpx;}
.count_control input{width:88rpx;text-align:center;height:60rpx;line-height:60rpx;border:none;border-left:1rpx solid #fff;border-right:1rpx solid #fff;}
.count_control.lit{top:0;width:202rpx;}
.count_control.lit input{width:58rpx;}
.count_control.lit em{width:70rpx;}
.count_control em.red{background:url('data:image/png;base64,') no-repeat center center;background-size:20rpx auto;}
.count_control em.red.disable{background:url('data:image/png;base64,') no-repeat center center;background-size:20rpx auto;}
.count_control em.add{background:url('data:image/png;base64,') no-repeat center center;background-size:20rpx 20rpx;}
.count_control em.add.disable{background:url('data:image/png;base64,') no-repeat center center;background-size:20rpx 20rpx;}
