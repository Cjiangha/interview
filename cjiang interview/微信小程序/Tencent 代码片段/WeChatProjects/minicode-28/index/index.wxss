.intro {
  margin: 30px;
  text-align: center;
}
.container{
  height: 100vh;
  width: 750rpx;
  display: flex;
  position: relative;
}
.map{
  height: 100%;
  width: 100%;
  position: absolute;
  z-index: 0;
}
.tap{
  display: flex;
  position: relative;
  top: 70vh;
  left: 32rpx;
  background: rgba(255, 235, 205, 0.726);
  height: 12vh;
  width: 20vh;
  z-index: 2;
  align-items: center;
  justify-content: center;
}