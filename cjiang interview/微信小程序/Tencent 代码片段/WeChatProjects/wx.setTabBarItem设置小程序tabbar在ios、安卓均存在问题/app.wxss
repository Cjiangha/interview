/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.row {
  display: flex;
  flex-direction: row;
}
.column {
  display: flex;
  flex-direction: column;
}
.justify-end {
  justify-content:flex-end;
}
.items-end {
  align-items: flex-end;
}
.justify-start {
  justify-content:flex-start;
}
.items-start {
  align-items: flex-start;
}
.justify-center {
  justify-content: center;
}
.items-center {
  align-items: center;
}
.justify-between {
  justify-content: space-between;
}
.self-center {
  align-self: center;
}
.self-start {
  align-self: flex-start;
}
.self-end {
  align-self: flex-end;
}
.relative {
  position: relative;
}
.absolute {
  position: absolute;
}
.full-width {
  width: 100%;
}
.full-height {
  height: 100%;
}
.over-visible {
  overflow: visible;
}
.over-hidden {
  overflow: hidden;
}