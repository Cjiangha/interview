/* pages/move/move.wxss */
@import "../lib/font/iconfont.wxss";
@import "../../common/font/iconfont.wxss";
.moveFather {
  width: 100%;
  height: 100%;

  box-sizing: border-box;
  position: relative;
  z-index: 0;
}

.movechild {
  width: 300rpx;
  height: 400rpx;

  z-index: 9999;
}
.unMove {
  width: 100%;
  height: 100%;

  z-index: 9999;
}
.my {
  height: 100%;
  width: 100%;
}
.big {
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: -1;
  position: relative;
}
.topView {
  position: fixed;
  bottom: 140rpx;

  display: flex;
  justify-content: space-between;
  align-items: center;
  z-index: 9999999;
}
.twoBar {
  left: 25%;
  width: 50%;
}
.threeBar {
  left: 10%;
  width: 80%;
}
.overpng {
  width: 88rpx;
  height: 88rpx;
  display: block;
  margin: 0 auto;
}
.bgpng {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}
.bgpng .waiting {
  width: 100%;
  height: 100%;
}
.container {
  height: 100%;
  overflow: hidden;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 99;
}
.optlook {
  opacity: 0;
  height: 0;
  width: 0;
  margin: 0;
  padding: 0;
}

.positop {
  position: absolute;
  top: 140rpx;
  width: 90%;
  left: 5%;
  height: 192rpx;
  background: #000000;
  opacity: 0.7;
  border-radius: 10rpx;
  z-index: 999;
}
.userLook {
  position: absolute;
  top: 140rpx;
  width: 90%;
  left: 5%;
  color: #fff;
  padding: 3%;
  box-sizing: border-box;
  z-index: 9999;
}
.userLook .close {
  display: flex;
  margin-bottom: 15rpx;
}
.userLook .close .s1 {
  width: 90%;
  text-align: center;
  font-size: 30rpx;
}
.userLook .close .s2 {
  width: 10%;
  color: FFF;
  text-align: center;
  font-size: 35rpx;
}
.camerafont {
  font-size: 32rpx;
  line-height: 42rpx;
}
.btfont {
  color: #fff;
  font-size: 24rpx;
  margin-top: 20rpx;
  text-align: center;
}

/* 征信授权 */
.text-center {
  text-align: center !important;
}

.weui-panel {
  background-color: #fff;
  padding: 30rpx;
}

.weui-panel image {
  width: 100rpx;
  height: 100rpx;
}

.person text {
  color: #666;
  font-size: 30rpx;
  text-indent: 10rpx;
}

.person {
  padding: 5%;
  background: #fff;
  margin-top: 20rpx;
}

.person .title {
  font-size: 34rpx;
  width: 100%;
  margin-bottom: 30rpx;
  display: block;
  line-height: 34rpx;
  text-align: center;
}
.person .indent {
  text-indent: 50rpx;
  color: #666;
  display: block;
}

.useragree {
  margin-top: 15rpx;
}

.useragree text {
  height: 50rpx;
  line-height: 50rpx;
  display: inline-block;
  font-size: 26rpx;
}

.btn-area {
  padding: 3%;
  margin-top: 60rpx;
}

.btnbgk {
  background: #d9d9d9 !important;
  color: #fff !important;
}

.person .lokiner {
  display: inline-block;
  font-size: 30rpx;
  height: 60rpx;
  line-height: 60rpx;
  margin-left: 10rpx;
  letter-spacing: 1rpx;
}

.inforpad {
  margin-top: 10rpx;
}

.person .otent {
  display: inline-block;
  font-size: 30rpx;
  height: 60rpx;
  line-height: 60rpx;
}

.inforpad {
  overflow: hidden;
}

.userIfo {
  display: flex;
  justify-content: flex-start;
}

.signature {
  position: absolute;
  width: 100%;
  height: 100%;
  border: 1px solid red;
  left: 100%;
  top: 0;
  box-sizing: border-box;
  background: #fff;
  transition: all 0.2s linear;
}

.inpic {
  left: 0;
}

.myCanvas {
  height: 1050rpx;
  border: 1px solid #d8d8d8;
  width: 100%;
  box-sizing: border-box;
}

.useragree {
  background: #fff;
  position: relative;
  overflow: hidden;
}

#cancas_btn {
  display: flex;
  justify-content: space-between;
  padding: 4% 0;
  box-sizing: border-box;
}

.line1 {
  position: absolute;
  top: 350rpx;
  height: 1rpx;
  width: 100%;
  border-top: 1px dashed #c8c8c8;
}

.line2 {
  position: absolute;
  top: 700rpx;
  height: 1rpx;
  width: 100%;
  border-top: 1px dashed #c8c8c8;
}

.coverbox {
  background: #fff;
  padding: 5% 10%;
  box-sizing: border-box;
}

button {
  box-sizing: border-box;
}

.canvas_btn {
  background: #f37938;
  color: #fff;
}

.checkbox {
  width: 100%;
  position: relative;
  display: block;
}

.useragree view {
  height: 50rpx;
  line-height: 50rpx;
  display: inline-block;
  font-size: 26rpx;
  color: #003100 !important;
}
#btnorigin {
  background: #f37938;
  color: #fff;
}
.md {
