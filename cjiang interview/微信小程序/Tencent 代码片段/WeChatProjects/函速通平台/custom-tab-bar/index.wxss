.tab-bar{position:fixed;bottom:0;left:0;right:0;height:48px;background:#fff;display:-webkit-box;display:-webkit-flex;display:flex;padding-bottom:env(safe-area-inset-bottom)}.tab-bar-border{background-color:rgba(0,0,0,.33);position:absolute;left:0;top:0;width:100%;height:1px;-webkit-transform:scaleY(.5);transform:scaleY(.5)}.tab-bar-item{-webkit-box-flex:1;-webkit-flex:1;flex:1;text-align:center;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center;-webkit-box-align:center;-webkit-align-items:center;align-items:center;-webkit-box-orient:vertical;-webkit-box-direction:normal;-webkit-flex-direction:column;flex-direction:column}.tab-bar-item cover-image{width:22px;height:22px}.tab-bar-item cover-view{font-size:10px}