/* pages/map/index.wxss */
page{
    width: 100%;
    height: 100%;
}
.map-h5{
    width: 100%;
    height: 100%;
}
.nav-back-box{
    position: absolute;
    z-index: 99;
    bottom: 0px;
    width: calc(100% - 40rpx);
    height: 120rpx;
    background-color: white;
    display: flex;
    align-items: center;
    padding: 0 20rpx;
}
.title{
    flex: 1;
    font-size: 32rpx;
}
.icon{
    width: 80rpx;
    height: 80rpx;
}
.back{
    position: absolute;
    z-index: 99;
    top: 24px;
    width: 26px;
    height: 26px;
    left: 20rpx;
}
.position{
    position: absolute;
    z-index: 99;
    bottom: 400rpx;
    right: 30rpx;
    width: 74rpx;
    height: 74rpx;
    background-color: white;
    border-radius: 50%;
    box-shadow: 0px 0px 20rpx rgba(0,0,0,0.3);
    opacity: 0.6;
    display: flex;
    align-items: center;
    justify-content: center;
}
.position-icon{
    width: 56rpx;
    height: 56rpx;
}