
.container{
  position: relative;
  width: 100%;
  height: 100vh;
  overflow:hidden ;
}
.bgImg{
  position: absolute;
  left: 0;
  width: 100%;
  height: 100%;
  right: 0;
  bottom: 0;
  top: 0;
  z-index: -1;
}
.main{
  margin-top: 146rpx;
  height:calc(100vh - 24px);
  position: relative;
  overflow-y: auto;
  padding-bottom: 26rpx;
}
.online-index-box{
  position: relative;
}
.main-top{
  width:700rpx;
  height:332rpx;
  margin: 0 25rpx;
  position: relative;
}
.online-top{
  width: 100%;
  height: 100%;
}
.main-title{
  position: absolute;
  width: 295rpx;
  height: 30rpx;
  top: 270rpx;
  left: 400rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  color: #B27205;
  z-index: 20;
}
.main-middle{
  height: 740rpx;
  width: 670rpx;
  margin: 0 47rpx 0 25rpx;
  position: relative;
  z-index: 0;
}
.record-txt{
  height: 127rpx;
  width: 510rpx;
  float: right;
  font-size: 26rpx;
  margin-right: 15rpx;
  letter-spacing: 6rpx;
  justify-content: flex-end;
  font-size: 34rpx;
  font-weight:blod;
  color: #fff;
}
.phcolor{
  color: #fff;
  letter-spacing: 6rpx;
}
.bbfff{
  border-bottom: 2rpx solid #fff;
  display: inline-block;
  padding: 4rpx 10rpx;
}
.record-inp{
  width: 510rpx;
  padding: 0 10rpx;
  
}
.main-middle .bg,.upload .upload-img{
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: -1;
}
.close{
  position: absolute;
  right: -20rpx;
  top: -20rpx;
  width: 46rpx;
  height: 46rpx;
}
.file-img{
  width: 100%;
  height: 100%;
}
.upload{
  width:500rpx;
  height: 516rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-46%);
}
.upload .upload-img{
  width:257rpx;
  height: 265rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  z-index: -1;
}
.icon-bor{
  width:194rpx;
  height: 226rpx;
  position: absolute;
  right: -42rpx;
}
.bortop1{
  top:170rpx
}
.bortop2{
  top:286rpx;
}
.upload-bottom{
  width: 707rpx;
  height: 205rpx;
  left:-12rpx;
  bottom: -64rpx;
  position: absolute;
}
.upload-bottom .upload-b-head{
  width: 100%;
  height:100%;
  position: absolute;
  top:0;
  left:0;
  z-index: 1;
}
.upload-bottom view{
  position: relative;
  z-index: 22;
}
.upload-b-txt{
  color: #fff452;
  font-size: 24rpx;
  text-shadow:2rpx 0 0px #901d1d, -2rpx 0 0px #901d1d, 0 2rpx #901d1d, 0 -2rpx #901d1d,2rpx 2px #901d1d, -2rpx -2rpx #901d1d, 2px -2rpx #901d1d, -2rpx 2rpx #901d1d;
}
.up-txt{
  color: #fff452;
  font-size: 30rpx;
}
.txt-rule1{
  margin: 75rpx 26rpx 0 210rpx;
}
.txt-rule3{
  margin: 90rpx 26rpx 0 210rpx;
}
.txt-rule2{
  margin: 4rpx 26rpx 0 186rpx;
}
/*  */
.main-bottom{
  width: 670rpx;
  height: 246rpx;
  margin: 50rpx 40rpx 0;
  position: relative;
  overflow: hidden;
}
.main-bottom-bg{
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  right: 0;
  z-index: -1;
}
.main-bot-title{
  font-size: 30rpx;
  color: #B27205;
  margin: 35rpx 0 8rpx 0;
  letter-spacing: 2rpx;
}
.main-bot-tips{
  font-size: 22rpx;
  color: #B27205;
  letter-spacing: 2rpx;
  line-height: 30rpx;
}
.main-bot-left{
  width: 154rpx;
  height: 84rpx;
  font-size: 30rpx;
  margin:0 26rpx  0 46rpx;
  color: #B27205;
  letter-spacing:4rpx;
  position: relative;
}
.main-bot-right{
  margin-right: 16rpx;
}
.mb-tip{
  width: 146rpx;
  height: 84rpx;
  line-height: 84rpx;
}
.main-bot-left image{
  width: 154rpx;
  height: 84rpx;
  position: absolute;
  left: 0;
  top: 0;
  z-index:-1
}
.txt-bg{
  font-size: 20rpx;
  background: #FFEB47;
  border-radius: 8rpx;
}
/*  */
.bottom-btns{
  margin: 20rpx 40rpx;
  display: flex;
}
.jcsb{
  justify-content: space-between;
}
.jcc{
  justify-content: center;
}

.main-btn{
  width:315rpx;
  height:100rpx;
  line-height: 84rpx;
  position: relative;

}
.main-btn text{
  background:linear-gradient(to bottom, #FF6D00, #FF4000);
  -webkit-background-clip: text;
  background-clip: text;
  letter-spacing: 6rpx;
  font-weight: bold;
  font-size: 40rpx;
  color: transparent;
}
.main-btn image{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: -1;
}
.c-box {
  height:100vh;
  overflow: hidden;
  position: fixed;
  top: 100rpx;
  top: -9999999rpx;
}
.fill-litte{
  width:396rpx;
  height: 68rpx;
  line-height: 68rpx;
  text-align: center;
  margin:20rpx 0 0 120rpx;
  color: #B91C1C;
  font-size: 28rpx;
  background:rgba(236,110,76,1);
  border-radius: 8rpx;
  position: relative;
}
.picker{
  position: relative;
  width: 100%;
  height: 100%;
}
.picker .icon-down{
  width: 20rpx;
  height: 20rpx;
  position: absolute;
  right: 20rpx;
  top: 50%;
  transform: translateY(-50%);
}
.mb66{
  margin-bottom: 66rpx;
}