.text_lins1 {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.text_lins2 {
  overflow: hidden;
  white-space: pre-line;
  text-overflow: ellipsis;
  display: -webkit-box;
