/**app.wxss**/

page {
  width: 100%;
  margin: 0;
  padding: 0;
  background-color: #f4f4f4;
  font-size: 26rpx;
  font-family: "微软雅黑";
}

.hide {
  display: none;
}

view {
  -moz-osx-font-smoothing: grayscale;
  -webkit-font-smoothing: antialiased;
}

img, image {
  width: 100%;
  height: 100rpx;
  vertical-align: middle;
}

view, text, img, image {
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  -webkit-user-select: none;
  outline: 0;
  -webkit-tap-highlight-color: transparent;
}

.wxParse-content {
  font-size: 28rpx;
  line-height: 50rpx;
  color: #666;
  overflow: hidden;
  padding: 20rpx 0;
}

.wxParse-content image {
  max-width: 100%;
}

.wxParse-content .wxParse-p {
  margin: 10rpx 0;
}

.shade_transparent {
  width: 100%;
  height: 100%;
  top: 0;
  background: rgba(0, 0, 0, 0);
  overflow: hidden;
  /*要显示在wrap的上面 */
  z-index: 1;
  position: fixed;
}

.shade {
  width: 100%;
  height: 100%;
  top: 0;
  background: rgba(0, 0, 0, 0.5);
  overflow: hidden;
  /*要显示在wrap的上面 */
  z-index: 1;
  position: absolute;
}

.cnvp-model {
  position: absolute;
  background: #fff;
  z-index: 100;
  left: 50%;
  top: 50%;
  width: 80%;
  height: auto;
  margin: -150rpx 0 0 -40%;
  display: flex;
  flex-direction: column;
  border-radius: 10rpx;
  padding: 30rpx;
  justify-content: space-between;
  text-align: center;
}

.cnvp-model .title {
  font-size: 18px;
  color: #333;
  line-height: 80rpx;
  font-weight: bold;
}

.cnvp-model .text {
  font-size: 16px;
  padding: 20rpx;
  color: #999;
  line-height: 66rpx;
}

.cnvp-model button {
  border: 0 solid #fff;
  width: 60%;
}

/* 清除左右对齐 */

.clear:after {
  content: "";
  clear: both;
  display: block;
}

.clear {
  content: "";
  clear: both;
  display: block;
}

.abs {
  position: absolute !important;
}

.rel {
  position: relative !important;
}

