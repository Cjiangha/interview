{
  "name": "nbshop-wxapp-shop",
  "version": "1.0.0",
  "private": true,
  "description": "新美小店C端",
  "templateInfo": {
    "name": "mobx",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build:wxTest": "taro build --type weapp --env testProd",
    "build:wxProd": "taro build --type weapp --env production",
    "build:wxDev": "taro build --type weapp --env development",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:wxDev -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "style": "stylelint --fix src/**/*.scss",
    "styleCheck": "stylelint-config-prettier-check",
    "lint": "eslint --fix src/**/*.{ts,tsx,js}",
    "format": "prettier --write src/**/*.{scss,ts,tsx,json,js}"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@tarojs/components": "3.3.0",
    "@tarojs/react": "3.3.0",
    "@tarojs/runtime": "3.3.0",
    "@tarojs/taro": "3.3.0",
    "mobx": "^4.8.0",
    "mobx-react": "^6.1.4",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "save": "^2.4.0",
    "wx-base64-qrcode": "^1.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@tarojs/mini-runner": "3.3.0",
    "@tarojs/webpack-runner": "3.3.0",
    "@types/react": "^17.0.2",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-preset-taro": "3.3.0",
    "eslint": "^6.8.0",
    "eslint-config-taro": "3.3.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "4.3.8",
    "lint-staged": "10.5.3",
    "prettier": "^2.2.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.2.0",
    "typescript": "^4.1.0",
    "webpack-merge": "^5.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{scss,css}": [
      "stylelint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
