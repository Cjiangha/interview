.intro {
  margin: 30px;
  text-align: center;
}
.map {
  width: 100%;
  height: 1000rpx;
}
.poiBox {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 100;
}
.poiList {
  height: 500rpx;
  background: rgba(0, 0, 0, 0.6);
  box-sizing: border-box;
  overflow-y: scroll;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
}
.item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #fff;
}