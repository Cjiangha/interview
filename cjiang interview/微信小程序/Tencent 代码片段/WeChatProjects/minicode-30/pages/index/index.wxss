/**index.wxss**/
.page{
  width: 100vw;
  height: 100vh
}
.page .map-box{
  width: 100%;
  height: 90%
}
.page .map{
  width: 100%;
  height: 100%
}
.page .remove{
  width: 200rpx;
  border: 4rpx solid #ccc;
  text-align: center
}
.page .move{
  width: 200rpx;
  border: 4rpx solid #ccc;
  text-align: center;
  margin-top:5rpx
}
.content{
  height: 96rpx;
  padding: 0 18rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 20rpx;
  color: #333;
  background-color: #FFF;
  line-height: 38rpx;
  border-radius: 16rpx;
}
.red{
  font-size: 28rpx;
  color: #E43F3F;
  padding: 0 4rpx;
}