.navar-wrap {
  position: relative;
  width: 100%;
  height: 60px;
}
.navbar {
  position: fixed;
  width: 100%;
  height: 60px;
  line-height: 60px;
  text-align: center;
  background-color: #ccc;
}
.item {
  height: 50px;
  line-height: 50px;
  text-align: center;
  margin-bottom: 10px;
  background-color: pink;
}
.btn {
  position: fixed;
  bottom: 20px;
  right: 20px;
  width: 50px;
  height: 50px;
  line-height: 50px;
  border-radius: 50%;
  font-size: 16px;
  text-align: center;
  background-color: red;
  color: #fff;
}
